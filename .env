port=3000
# express.js variable - can be set to `production` or `development`
NODE_ENV=development
# Terra network that is used - available options - 'localterra', 'testnet', 'mainnet'
network=testnet

# SET ME BELOW!

# Only bot with this token is able to communicate with backend - set it to your bot
bot_token=
# Full mongoURI to your DB - see https://docs.mongodb.com/manual/reference/connection-string/
mongo_uri=
# Mnemonic of your Terra wallet - it is used to broadcast transactions on behalf of the user
mnemonic=
